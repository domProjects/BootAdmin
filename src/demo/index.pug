include variables/versions.pug

include variables/mixins.pug

- var brand = "BootAdmin";
- var title = "Dashboard";

doctype html
html(lang='en')

	head
		meta(charset='utf-8')
		meta(name='viewport', content='width=device-width, initial-scale=1')
		meta(name='color-scheme', content='light dark')

		title #{title} - #{brand}

		// stylesheet
		include content/style.pug
	body

		// header
		header#header
			#header-title
				span #{brand}

			#header-content
				#header-action
					include content/header-action.pug

				#header-info
					include content/header-info.pug

		// content
		#content

			// nav
			nav#nav
				include content/nav.pug

			// main
			main#main

				// Breadcrumb
				nav(aria-label='breadcrumb')
					ol.breadcrumb
						li.breadcrumb-item
							a(href='index.html') Dashboard
						li.breadcrumb-item
							a(href='index.html') Bootstrap
						li(class='breadcrumb-item active', aria-current='page') Accordion

				// section
				section#section
					.container-fluid
