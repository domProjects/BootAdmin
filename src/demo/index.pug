										h5.card-title 225
										.progress
											.progress-bar(role='progressbar' style='width: 50%;' aria-valuenow='50' aria-valuemin='0' aria-valuemax='100')
										p.card-text 50% of incidents resolved
						.row
							.col-md-6
								#fs-001.card.card-tools.isFullscreen
									.card-header
										.card-header-title Featured
										.card-header-tools
											.btn-group.btn-group-sm(role='group' aria-label='Tools')
												button.btn.btn-sm.btn-tools.btn-fullscreen(type='button')
												button.btn.btn-sm.btn-tools.btn-collapse(type='button' data-bs-toggle='collapse' data-bs-target='#aaaaa' aria-expanded='true' aria-controls='aaaaa')
									#aaaaa.collapse.show
										.card-body
											h5.card-title Special title treatment
